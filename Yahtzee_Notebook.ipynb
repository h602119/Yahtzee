    "        if inf > topInf:\n",
    "            keepList = kL\n",
    "            topInf = inf\n",
    "    dice = reroll(dice, keepList)\n",
    "\n",
    "    # Do inference on all combinations of \"keep or discard\" with the current dice\n",
    "    for combination in combinations_dict.items():\n",
